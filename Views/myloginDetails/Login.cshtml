@model MVC_login.Models.myloginEntity

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Login</title>
    
</head>
<body>
    <h1 >Login</h1>
    
        @using (Html.BeginForm())
        {
    <div>
        UserID:
        <br />
        @Html.TextBoxFor(m => m.UserID)
        <br /><br />
        Password:
        <br />
        @Html.TextBoxFor(m => m.Password)
        <br /><br />
        <input type="submit" value="Login" name="button" />
       
    </div>
        }
</body>
</html>
